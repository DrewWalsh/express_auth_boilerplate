{
	"name": "express-auth-boilerplate-api",
	"version": "0.0.1",
	"description": "",
	"main": "app.js",
	"scripts": {
		"start": "nodemon --delay 500ms --exec babel-node src/app.js",
		"s": "yarn start",
		"migrate": "npx sequelize-cli db:migrate",
		"migrate:reset": "npx sequelize-cli db:migrate:undo:all && yarn migrate",
		"seed-all": "npx sequelize-cli db:seed:all",
		"test": "cross-env NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --runInBand --forceExit --testTimeout=10000 ----detectOpenHandles --logHeapUsage",
		"build": "rm -rv -f build && babel src --out-dir build",
		"push-to-production": "scp -r build/* drew@192.241.200.86:/var/www/domainname.ca",
		"wipe-for-update": "sudo rm -rv database dist helpers middleware modules app.js config.js logger.js"
	},
	"author": "Drew Walsh",
	"license": "ISC",
	"jest": {
		"testEnvironment": "node",
		"coveragePathIgnorePatterns": [
			"/node_modules/"
		]
	},
	"dependencies": {
		"bcrypt": "^5.1.0",
		"cheerio": "^1.0.0-rc.12",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"cron": "^2.3.0",
		"cross-fetch": "^3.1.5",
		"disconnect": "^1.2.2",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-brute": "^1.0.1",
		"express-brute-sequelize": "^0.1.0",
		"helmet": "^6.0.1",
		"joi": "^17.9.1",
		"jsonwebtoken": "^9.0.0",
		"log4js": "^6.9.1",
		"morgan": "^1.10.0",
		"multer": "^1.4.5-lts.1",
		"nodemailer": "^6.9.1",
		"object-hash": "^3.0.0",
		"pg": "^8.10.0",
		"pg-hstore": "^2.3.4",
		"sequelize": "^6.30.0",
		"sequelize-cli": "^6.6.0",
		"webp-middleware": "^0.4.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.3",
		"@babel/node": "^7.20.7",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.21.0",
		"@babel/preset-env": "^7.20.2",
		"@babel/runtime": "^7.21.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"babel-plugin-transform-require-default": "^0.1.7",
		"cross-env": "^7.0.3",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-loader": "^4.0.2",
		"eslint-plugin-jest": "^27.2.1",
		"eslint-plugin-prettier": "^4.2.1",
		"jest": "^29.5.0",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.7",
		"supertest": "^6.3.3"
	}
}
